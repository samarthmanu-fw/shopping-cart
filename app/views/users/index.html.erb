<h1>Users#index</h1>

<%= link_to 'New User', new_user_path %>

<table>
  <thead>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>is_admin</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.is_admin %></td>
    </tr>
  <% end %>
  </tbody>
</table>